<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="deckpackage.controller.QuoteIndexController">
   <children>
      <VBox prefHeight="500.0" prefWidth="900.0">
         <children>
            <Pane fx:id="topMenuPane" prefHeight="79.0" prefWidth="900.0" />
            <ScrollPane prefHeight="421.0" prefWidth="900.0">
               <content>
                  <TableView fx:id="quoteTableView" prefHeight="418.0" prefWidth="900.0">
                    <columns>
                      <TableColumn prefWidth="150.0" text="Customer ">
                        <cellValueFactory><PropertyValueFactory property="quoteName" /></cellValueFactory>
                      </TableColumn>
                        <TableColumn prefWidth="150.0" text="Date">
                        <cellValueFactory><PropertyValueFactory property="quoteDate" /></cellValueFactory>
                        </TableColumn>
                        <TableColumn prefWidth="75.0" text="Status ">
                        <cellValueFactory><PropertyValueFactory property="quoteStatus" /></cellValueFactory>
                        </TableColumn>
                        <TableColumn fx:id="apptColumn" prefWidth="150.0" sortable="false" text="Appointment">
                           <cellValueFactory>
                              <PropertyValueFactory property="apptButton" />
                           </cellValueFactory>
                        </TableColumn>
                    </columns>
                  </TableView>
               </content></ScrollPane>
         </children>
      </VBox>
   </children>
</AnchorPane>
